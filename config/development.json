{
    "filename": "development.json",
    
    "logging": {
    
        "winston": {
            
            "level": "debug",
            "useColors": true,
            "filenames": {
                "useDatedFilenames": false,
                "dateFormatStr": "yyyymmdd"

            },

            "transports": {
                "file": {
                    "general" : {
                        "level" : "debug"
                    }
                },
                "console": {
                    "general" : {
                        "level" : "debug",
                        "timestamp": true
                    }
                }
            }

        }
   	}
}

