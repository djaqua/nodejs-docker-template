{
    "filename": "development.json",

    "service": {
        "port": 8080
    }, 

    "logging": {
    
        "winston": {
            
            "level": "debug",
            "useColors": true,
            "filenames": {
                "useDatedFilenames": false,
                "dateFormatStr": "yyyymmdd"
            },

            "transports": {
                "file": {
                    "general" : {
                        "level" : "debug"
                    }
                },
                "console": {
                    "general" : {
                        "level" : "debug",
                        "timestamp": true
                    }
                }
            }
        }
   	}
}

